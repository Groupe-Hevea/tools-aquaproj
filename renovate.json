{
  "extends": [
    "config:recommended",
    "github>aquaproj/aqua-renovate-config#2.7.4",
    "github>aquaproj/aqua-renovate-config:file#2.7.4(CONTRIBUTING\\.md)",
    "github>aquaproj/aqua-renovate-config:file#2.7.4(aqua-all\\.yaml)",
    "github>aquaproj/aqua-renovate-config:file#2.7.4(aqua/.*\\.yaml)",
    "github>aquaproj/aqua-renovate-config:file#2.7.4(aqua-config/.*\\.yaml)",
    "github>aquaproj/aqua-renovate-config:file#2.7.4(pkgs/.*/pkg\\.yaml)",
    "github>suzuki-shunsuke/renovate-config#3.0.2"
  ],
  "automerge": true,
  "automergeType":  "branch",
  "dependencyDashboard": true,
  "platformAutomerge": true,
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "timezone": "Europe/Paris",
  "branchConcurrentLimit": 0,
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["README.md"],
      "matchStrings": [
        "ref: (?<currentValue>.*?) # renovate: depName=(?<depName>.*)"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
