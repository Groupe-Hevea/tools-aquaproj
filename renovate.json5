{
  extends: [
    "config:base",
    "github>aquaproj/aqua-renovate-config#1.13.0",
    "github>aquaproj/aqua-renovate-config:file#1.13.0(CONTRIBUTING\\.md)",
    "github>aquaproj/aqua-renovate-config:file#1.13.0(aqua-all\\.yaml)",
    "github>aquaproj/aqua-renovate-config:file#1.13.0(aqua/.*\\.yaml)",
    "github>aquaproj/aqua-renovate-config:file#1.13.0(aqua-config/.*\\.yaml)",
    "github>aquaproj/aqua-renovate-config:file#1.13.0(pkgs/.*/pkg\\.yaml)",
    "github>suzuki-shunsuke/renovate-config:action-semver#2.2.0",
    "github>suzuki-shunsuke/renovate-config:renovate-config-preset#2.2.0",
  ],
  automerge: true,
  platformAutomerge: true,
  prHourlyLimit: 0,
  prConcurrentLimit: 0,
  branchConcurrentLimit: 0,
  regexManagers: [
    {
      fileMatch: ["README.md"],
      matchStrings: [
        "ref: (?<currentValue>.*?) # renovate: depName=(?<depName>.*)",
      ],
      datasourceTemplate: "github-releases",
    },
  ],
}
